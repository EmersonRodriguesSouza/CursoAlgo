algoritmo "contador_solicitado

var
    contador,valor, salto :inteiro
inicio

       contador <- valor
       Escreva ("Quer contar até quanto?")
       Leia (valor)
       Escreva ("Qual sera o valor do salto")
       Leia (salto)
       Enquanto (contador <= valor) Faca
       EscrevaL (contador)
       contador <- contador + salto
       FimEnquanto
fimalgoritmo
