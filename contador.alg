algoritmo "semnome"

var
salto, contador, Quantidade:inteiro
inicio
contador <- Quantidade
Escreva ("Ate quanto: ")
Leia (Quantidade)
Escreva ("Quantidade de saltos: ")
Leia (salto)

Enquanto (Contador <= Quantidade) faca
   Escreval(contador)
   contador <- contador + salto
FimEnquanto
fimalgoritmo
