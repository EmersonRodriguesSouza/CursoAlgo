algoritmo "contadorNumérico"

var cont,N, S: inteiro

inicio
      cont <- 1
      S <- 0
      Enquanto (cont <=10) faca
      Escreva("Didite o", cont, "o. valor : ")
      Leia (N)
      S <- S + N
      cont <- cont + 1
      FimEnquanto
      EscrevaL ("A soma de todos os valores = ", S)

fimalgoritmo
